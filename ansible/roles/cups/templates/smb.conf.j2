[global]
workgroup = {{ cups_samba_workgroup }}
server string = {{ cups_samba_server_string }}
security = user
printing = cups
printcap name = cups
load printers = yes
cups options = raw

[printers]
comment = All Printers
path = /var/spool/samba
browseable = yes
guest ok = yes
writable = no
printable = yes
print ok = yes
use client driver = no
